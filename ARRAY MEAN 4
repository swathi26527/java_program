import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int sum = 0;

        for (int i = 0; i < n; i++) {
            int value = sc.nextInt();
            sum += value;
        }

        float mean = (float) sum / n;
        System.out.printf("Array Mean Value is %.2f\n", mean);
    }
}
