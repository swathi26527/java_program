import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String s = sc.next();
        if (s.length() < 2) {
            System.out.println("STDOUT");
        } else {
            char[] a = s.toCharArray();
            char temp = a[0];
            a[0] = a[1];
            a[1] = temp;
            System.out.println(Integer.parseInt(new String(a)));
        }
        sc.close();
    }
}
